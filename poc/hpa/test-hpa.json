{
  "apiVersion": "extensions/v1beta1",
  "kind": "HorizontalPodAutoscaler",
  "metadata": {
    "name": "gyliu-nginx",
    "namespace": "default"
  },
  "spec": {
    "scaleRef": {
      "kind": "ReplicationController",
      "name": "liqlin-nginx",
      "subresource": "scale"
    },
    "minReplicas": 1,
    "maxReplicas": 20,
    "cpuUtilization": {
      "targetPercentage": 20
    }
  }
}
